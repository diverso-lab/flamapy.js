{
  "name": "@lbdudc/flamapy.js",
  "version": "0.0.3",
  "description": "Instala wheels en Pyodide desde Node.js",
  "main": "lib/flamapy.js",
  "module": "lib/flamapy.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "npm run build:clean && vite build && npm run build:python",
    "build:clean": "node -e \"require('fs').rmdirSync('dist', { recursive: true })\"",
    "build:python": "node -e \"require('fs').cpSync('lib/py', 'dist/py', { recursive: true })\"",
    "dev": "vite"
  },
  "dependencies": {
    "pyodide": "0.26.2"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^14.0.0",
    "vite": "^4.0.0",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "author": "Diverso Lab",
  "contributors": [
    {
      "name": "Victor Lamas",
      "email": "victor.lamas@udc.es"
    },
    {
      "name": "David Organvidez",
      "email": ""
    }
  ],
  "license": "MIT"
}